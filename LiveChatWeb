	function initEmbeddedMessaging() {
		try {
			embeddedservice_bootstrap.settings.language = 'en_US'; // For example, enter 'en' or 'en-US'

			embeddedservice_bootstrap.init(
				'00DcZ000000Cxyp',
				'WebchatLive',
				'https://foodja--partial.sandbox.my.site.com/ESWWebchatLive1736400295448',
				{
					scrt2URL: 'https://foodja--partial.sandbox.my.salesforce-scrt.com'
				}
			);
		} catch (err) {
			console.error('Error loading Embedded Messaging: ', err);
		}
	};
</script>
<script type='text/javascript' src='https://foodja--partial.sandbox.my.site.com/ESWWebchatLive1736400295448/assets/js/bootstrap.min.js' onload='initEmbeddedMessaging()'></script>

